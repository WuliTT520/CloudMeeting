<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhihui.imeeting.cloudmeeting">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/logo144px"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo1024px"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.ForgetActivity" />
        <activity android:name=".activity.SetPasswordActivity" />
        <activity android:name=".activity.FaceIDActivity" />
        <activity android:name=".activity.AddActivity" />
        <activity android:name=".activity.GroupActivity" />
        <activity android:name=".activity.GroupInfoActivity" />
        <activity android:name=".activity.GroupAddActivity" />
        <activity android:name=".activity.GroupSetActivity" />
        <activity android:name=".activity.UserInfoActivity" />
        <activity android:name=".activity.SetPhoneActivity" />
        <activity android:name=".activity.SetResumeActivity" />
        <activity android:name=".activity.FaceManagerActivity" />
        <activity android:name=".activity.ChangePwdActivity" />
        <activity android:name=".activity.MyReserveActivity" />
        <activity android:name=".activity.AttendeeActivity" />
        <activity android:name=".activity.MyGroupActivity" />
        <activity android:name=".activity.ShowGroupInfoActivity" />
        <activity android:name=".activity.ChooseBoardroomActivity"></activity>
    </application>

</manifest>